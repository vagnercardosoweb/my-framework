{% extends '@error/app.twig' %}
{% set error_title = 'Error 404 (Not Found)' %}

{% block content %}
  <div class="error-404">
    <div class="content">
      <h1>Página não encontrada.</h1>
      <p>{{ url|raw }}</p>
      <a href="{{ constant('BASE_URL') }}">Voltar</a>
    </div>
  </div>
{% endblock %}
